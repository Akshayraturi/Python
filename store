op=str(input("enter the operator"))
a=float(input("enter the first number"))
b=float(input("enter the second number"))
if op=="+":
  print(a,"+",b,"=",a+b)
  if op=="-":
    print(a,"-",b,"=",a-b)
    if op=="*":
      print(a,"*",b,"=",a*b)
      if op=="/":
        print(a,"/",b,"=",a/b)
