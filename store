class Person:
    def __init__(self):
        pass
    def sum(self,a,b):
        print("The sum of these numbers are",a+b)
    def subtract(self,a,b):
        print("The subtraction of these numbers are",a-b)
    def divide(self,a,b):
        print("The subtraction of these numbers are",a-b) 
    
obj=Person()
obj2=Person()
obj.car(str(input("enter the car's brand")))
obj.myname("akshay")
