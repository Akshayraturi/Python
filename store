class Person:
        def __init__(self):
                pass
        def myname(self,name):
                print("My name is:",name)
        def my_address(self,address):
                print("My address is: ",address)
        def my_aadhar(self,aadhar):
                print("My aaharcard number is: ",aadhar)
        def mobile(self,number):
                print("My phone number is: ",number)

        
class Employee(Person):
        
        def __init____(self):
                pass
        def salary(self,salary):
                print("My salary is: ",salary)
        def id(self,id):
                print("My id is: ",id)
        def post(self,post):
                        print("I am :",post,"in the organization")
akshay=Employee()
akshay.myname("akshay")
akshay.my_address("rishikesh,uttarakhand")
akshay.my_aadhar(6456565645646)
akshay.mobile(995342244)
akshay.salary(100000)
akshay.post("IAS")
akshay.id("admin@453")
